        <img src="~/Img/music/beg.png" style="width: 5%; height: 5%; " onclick="document.getElementById('audio').play(); return false;" id="beg" />
        <img src="~/Img/music/end.png" style="width:  5%; height: 5%;display: none " onclick="document.getElementById('audio').pause(); return false;" id="end" />

