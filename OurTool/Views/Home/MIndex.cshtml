<link href="~/Static/our666/bd/style.css" rel="stylesheet" />
<style type="text/css">
    .bbtn {
        cursor: pointer;
        width: 100px;
        height: 36px;
        color: white;
        font-size: 15px;
        letter-spacing: 1px;
        background: #3385ff;
        border-bottom: 1px solid #2d78f4;
        outline: medium;
    }

    body iframe {
        overflow-y: scroll;
    }
</style>


<script type="text/javascript">
    //回车事件
    $(function () {
        $('body').bind('keypress', function (event) {
            if (event.keyCode == "13") {
                $("#form").submit();//进行提交表单

            }
        });
    });
</script>


@*//scroll*@
<div style="overflow-x: hidden;">
